<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1dp" android:color="@color/textColor_teshe_half" />
            <corners android:radius="4dp" />
            <solid android:color="@color/white" />
            <padding android:left="8dp" android:right="8dp" android:top="4dp" android:bottom="4dp"/>

        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <stroke android:width="1dp" android:color="@color/textColor_teshe" />
            <corners android:radius="4dp" />
            <solid android:color="@color/white" />
            <padding android:left="8dp" android:right="8dp" android:top="4dp" android:bottom="4dp"/>

        </shape>
    </item>
</selector>