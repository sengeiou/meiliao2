<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:app="http://schemas.android.com/apk/res-auto">

    <style name="action_bar">
        <item name="android:background">@color/colorPrimary</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
    </style>

    <style name="hor_match_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.8dp</item>
        <item name="android:background">@color/divider1</item>
    </style>

    <style name="hor_match_line_margin_left" parent="hor_match_line">
        <item name="android:layout_marginLeft">12dp</item>
    </style>


    <style name="hor_match_line_8dp">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">8dp</item>
        <item name="android:background">@color/divider1</item>
    </style>
    <style name="hor_match_line_4dp">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">4dp</item>
        <item name="android:background">@color/divider1</item>
    </style>



    <style name="hor_match_dash_line">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_margin">2dp</item>
        <item name="android:background">@drawable/shape_get_video_coupon_line</item>
    </style>

    <style name="SmallLineWeightView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0.8dp</item>
        <item name="android:layout_weight">1</item>
    </style>


    <style name="CommonBtnStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/item_common_height</item>
        <item name="android:background">@drawable/selector_login_ok</item>
        <item name="android:textColor">@drawable/selector_edittext_gray_gray3</item>
        <item name="android:textSize">36px</item>
    </style>

    <style name="CommonEditTextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/item_common_height</item>
        <item name="android:background">@drawable/shape_edittext_bg</item>
        <item name="android:textColor">@color/textColor_333</item>
        <item name="android:textColorHint">@color/textColor_hint</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">36px</item>
    </style>

    <style name="CommonListViewStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:dividerHeight">0.8dp</item>
        <item name="android:divider">#0a0d0c</item>
    </style>

    <style name="CommonTextViewStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/textColor_333</item>
        <item name="android:textSize">18sp</item>
    </style>


    <style name="TTNotificationText">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>

    <style name="TTNotificationTitle">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>

    <style name="TextViewLabelStyle">
        <item name="android:layout_width">@dimen/lable_width</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:text">VIP</item>
        <item name="android:textColor">@color/textColor_teshe</item>
        <item name="android:background">@drawable/shape_tv_jyg_vip_bg</item>
    </style>

    <style name="TextViewLabelStyleRight" parent="TextViewLabelStyle">
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">12dp</item>
    </style>

    <style name="BottomShareImageView" parent="SmallLineWeightView">
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="ChatCustomView">
        <item name="android:layout_width">@dimen/chat_width</item>
        <item name="android:layout_height">@dimen/chat_height</item>
    </style>


    <style name="SJKFullLiveBtn">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">24dp</item>
        <item name="android:paddingRight">24dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="BottomPayView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_marginLeft">24dp</item>
        <item name="android:layout_marginRight">24dp</item>
        <item name="android:layout_marginTop">@dimen/edge_margin</item>
        <item name="android:background">@drawable/selector_btn_down</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/white</item>
    </style>


    <style name="LoginTabBtn">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@drawable/selector_write_write70</item>
    </style>


    <style name="EditUserInfoEditText" parent="CommonEditTextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">16dp</item>
    </style>

    <style name="EXTCameraStreamingImageView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">8dp</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>


    <style name="weight_space">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="MoreImageView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="ShareAlbumImageView" parent="MoreImageView">
        <item name="android:padding">10dp</item>
    </style>


    <style name="GainCouponImageView" parent="MoreImageView">
        <item name="android:layout_marginLeft">4dp</item>
        <item name="android:layout_marginRight">4dp</item>
        <item name="android:scaleType">centerCrop</item>
        <item name="android:src">@mipmap/ic_loading_square</item>
    </style>


    <style name="UserInfoItemGroupView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/item_common_height</item>
        <item name="android:paddingLeft">@dimen/edge_margin</item>
    </style>

    <style name="WebHeadImageView">
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">24dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">12dp</item>
    </style>


    <style name="BottomDownloadButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:background">@drawable/selector_btn_down</item>
        <item name="android:textColor">@color/white</item>
    </style>


    <style name="MainSJKCourseHeadTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_below">@+id/iv_main_sjk_course_head_pic</item>
        <item name="android:gravity">center</item>
        <item name="android:drawablePadding">8dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/textColor_666</item>
    </style>

    <style name="LinearLayoutDivider">
        <item name="android:divider">@drawable/selector_linearlayout_divider</item>
        <item name="android:showDividers">middle</item>
        <item name="android:dividerPadding">24dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:orientation">vertical</item>
    </style>


    <style name="BaseItemView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:minHeight">@dimen/item_common_small_height</item>
        <item name="android:layout_height">@dimen/item_common_small_height</item>
    </style>

    <style name="BaseItemViewWrap" parent="BaseItemView">
        <item name="android:layout_height">wrap_content</item>
    </style>


    <style name="smoothCheckBox">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
    </style>


    <style name="TextViewTitle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">1</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:layout_marginRight">12dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/textColor_333</item>
        <item name="android:textColorHint">@color/textColor_333</item>
    </style>

    <style name="TextViewContent">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">top</item>
        <item name="android:minLines">2</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/textColor_666</item>
        <item name="android:textColorHint">@color/textColor_666</item>
    </style>

</resources>